@{
    ViewBag.Title = "Submission Successful";
    Layout = "~/Views/Shared/_LayoutGeneral.cshtml";
}

<div class="container pt-2">
    <div class="row">

        <div class="col-md-12">
            <div class="card p-2">
                <div class="notfound-box">
                    <h1><span class="fa fa-smile-o"></span></h1>

                    <h3>Yippee... Submission Successful!</h3>
                    <h5>Your PackageID is <strong class="text-success">@ViewBag.eqcode</strong></h5>
